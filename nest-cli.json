{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gardone/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gardone/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gardone",
  "projects": {
    "gardone": {
      "type": "application",
      "root": "apps/gardone",
      "entryFile": "main",
      "sourceRoot": "apps/gardone/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gardone/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "libs\\": {
      "type": "application",
      "root": "apps/libs",
      "entryFile": "main",
      "sourceRoot": "apps/libs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.app.json"
      }
    },
    "libs": {
      "type": "library",
      "root": "libs/libs",
      "entryFile": "index",
      "sourceRoot": "libs/libs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/libs/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "referral-service": {
      "type": "application",
      "root": "apps/referral-service",
      "entryFile": "main",
      "sourceRoot": "apps/referral-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/referral-service/tsconfig.app.json"
      }
    },
    "socre-microservice": {
      "type": "application",
      "root": "apps/socre-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/socre-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/socre-microservice/tsconfig.app.json"
      }
    }
  }
}